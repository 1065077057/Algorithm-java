#include<iostream>
#include<string>

using namespace std;

int tree[100010];
// 关于N，实际上N是跟实际数组长度有关，可以每添加一次就令N++,不能直接让N取极大的定值
int N = 100005;

void update(int index,int n){
	for(int i = index;i < N;i += (i&-i)){
		tree[i] += n;
	}
}

int query_sum(int n){
	int ans = 0;
	for(int i = n;i > 0;i -= (i&-i)){
		ans += tree[i];
	}
	return ans;
}

int main() {
    
    system("pause");
    return 0;
}
